(import-all "../function")

(import-all "list-fold-left")

(assert-equal 10 (list-fold-left iadd 0 (@tael 1 2 3 4)))

(assert-equal (@tael 4 3 2 1)
 (list-fold-left (swap cons) (@tael) (@tael 1 2 3 4)))

(assert-equal (@tael 4 3 2 1)
 ((specific list-fold-left int? (list? int?)) (swap cons)
  (@tael)
  (@tael 1 2 3 4)))

