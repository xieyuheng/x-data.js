(import-all "list-append")

(assert-equal
  (list-append (@tael 1 2 3) (@tael 4 5 6))
  (@tael 1 2 3 4 5 6))

(assert-equal
  ((specific list-append int?) (@tael 1 2 3) (@tael 4 5 6))
  (@tael 1 2 3 4 5 6))

(assert-equal
  (list-append (@tael :x 1 :y 2 :z 3 1 2 3)
   (@tael :x 0 4 5 6))
  (@tael 1 2 3 4 5 6))

