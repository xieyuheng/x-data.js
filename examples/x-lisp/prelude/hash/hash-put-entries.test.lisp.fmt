(import-all "hash-put-entries")

(assert-equal (@hash (@quote a) 1 (@quote b) 2)
 (hash-put-entries
  (@tael (@tael (@quote a) 1) (@tael (@quote b) 2))
  (@hash)))

(begin
  (= hash (@hash))
  (assert-equal (@hash (@quote a) 1 (@quote b) 2)
   (hash-put-entries!
    (@tael (@tael (@quote a) 1) (@tael (@quote b) 2))
    hash))
  (assert-equal (@hash (@quote a) 1 (@quote b) 2) hash))

