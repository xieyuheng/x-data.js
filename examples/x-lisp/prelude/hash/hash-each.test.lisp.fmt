(import-all "hash-each")

(begin
  (= hash (@hash 1 2 3 4)) (= list (@tael))
  (hash-each-value (lambda (value) (list-push! value list))
   hash)
  (assert-equal (@tael 2 4) list))

(begin
  (= hash (@hash 1 2 3 4)) (= list (@tael))
  (hash-each-key (lambda (key) (list-push! key list)) hash)
  (assert-equal (@tael 1 3) list))

(begin
  (= hash (@hash 1 2 3 4)) (= list (@tael))
  (hash-each
   (lambda (key value)
     (list-push! key list) (list-push! value list))
   hash)
  (assert-equal (@tael 1 2 3 4) list))

