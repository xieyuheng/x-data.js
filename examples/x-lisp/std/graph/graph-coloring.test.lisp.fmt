(import-all "index")

(begin
  (= graph (make-graph (@tael) (@tael)))
  (graph-add-edge! (@tael "a" "b") graph)
  (graph-add-edge! (@tael "b" "c") graph)
  (graph-add-edge! (@tael "c" "d") graph)
  (graph-add-edge! (@tael "d" "a") graph)
  (= coloring (@hash))
  (= vertices (set-to-list (graph-vertices graph)))
  (assert-equal 2
   (pipe graph
     (graph-coloring! coloring vertices) hash-values
     list-to-set
     set-size)))

(begin
  (= graph (make-graph (@tael) (@tael)))
  (graph-add-edge! (@tael "d" "e") graph)
  (graph-add-edge! (@tael "a" "b") graph)
  (graph-add-edge! (@tael "b" "c") graph)
  (graph-add-edge! (@tael "c" "a") graph)
  (graph-add-edge! (@tael "b" "c") graph)
  (graph-add-edge! (@tael "c" "d") graph)
  (graph-add-edge! (@tael "d" "b") graph)
  (= coloring (@hash))
  (= vertices (set-to-list (graph-vertices graph)))
  (assert-equal 3
   (pipe graph
     (graph-coloring! coloring vertices) hash-values
     list-to-set
     set-size)))

